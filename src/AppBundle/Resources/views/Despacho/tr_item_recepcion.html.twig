<tr class="item" data-id="{{item.id}}" style="height:30px;">
    <td class="ordTd"></td>
    <td class="prodTd">{{item.producto.codigoNombre}}  {{item.lotesItemTxt|raw}}   </td>
    <td class="solTd alignright">{% if item.pedidoDetalle %}{{item.pedidoDetalle.cantidadTxt}}{%endif%} </td>
    <td class="cantTd alignright">{{ item.cantidadItemTxt }} </td>
    <td class="recTd alignright" {% if item.cantidadTotal!=item.entregado %} style="color:red" {% endif %}>{{item.recibidoTxt}}</td>
    <td style="text-align:center;width:20px;">
        <a class="editTd" href="javascript:void(0)" title="Editar" tabIndex="-1" 
           data-url="{{path('stock_render_edit_item_recepcion',{'id':item.id})}}" data-actionurl="{{path('stock_item_recepcion_update',{'id':item.id})}}">
            <img src="{{asset('assets/images/icons/editor.png')}}" /></a>
    </td>    
    <td style="text-align:center;"><a class="delTd" href="javascript:void(0)" data-id="{{item.id}}"  title="Marcar como no recibido" tabIndex="-1"><span class="minus"></span></a></td>
</tr>